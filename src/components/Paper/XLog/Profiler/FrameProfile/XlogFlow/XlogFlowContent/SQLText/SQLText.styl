.sql-text
  width: calc(100% - 12px)
  height: calc(100% - 50px)
  margin-top : 10px
  &::-webkit-scrollbar
    width: 7px
    height: 7px
  .prefix
    color: darkgreen !important
  .sql-statement
    line-height: 140%
    color: #0000C3
    white-space: nowrap
    overflow-x: auto
    width: 100%
  .formatter
    white-space : pre
